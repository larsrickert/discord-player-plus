{
  "name": "discord-player-plus",
  "description": "Library for quickly creating discord music and sound bots using discord.js v14.",
  "version": "1.4.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "homepage": "https://discordplayerplus.lars-rickert.de",
  "packageManager": "pnpm@7.27.0",
  "files": [
    "lib",
    "CHANGELOG.md"
  ],
  "author": {
    "name": "Lars Rickert",
    "url": "https://lars-rickert.de",
    "email": "dev@lars-rickert.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/larsrickert/discord-player-plus"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint --fix src --ignore-path .gitignore",
    "format": "prettier --write src --ignore-path .gitignore",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "discord.js": "^14.5.0",
    "ffmpeg-static": "^5.1.0"
  },
  "dependencies": {
    "deepmerge-ts": "^4.2.2",
    "isomorphic-unfetch": "^3.1.0",
    "libsodium-wrappers": "^0.7.10",
    "music-metadata": "^7.12.6",
    "play-dl": "^1.9.6",
    "spotify-url-info": "~3.1.10",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.3",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitest/coverage-c8": "^0.26.2",
    "discord-api-types": "^0.37.33",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "semantic-release": "^20.1.0",
    "typescript": "^4.9.4",
    "vitepress": "^1.0.0-alpha.33",
    "vitest": "^0.26.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write --ignore-path .gitignore"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "release": {
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "keywords": [
    "music",
    "player",
    "bot",
    "discord",
    "discord.js",
    "music"
  ]
}
